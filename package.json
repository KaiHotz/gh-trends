    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "check-types": "tsc",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
